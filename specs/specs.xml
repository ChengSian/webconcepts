<?xml version="1.0" encoding="UTF-8"?>
<specs>
    <organization id="IETF" href="http://www.ietf.org/">
        <name short="IETF">Internet Engineering Task Force</name>
        <series id="RFC">
            <name short="RFC">Request for Comments</name>
            <id-pattern url-pattern="http://tools.ietf.org/html/rfc$1" name-pattern="RFC $1" md-pattern="$1">urn:ietf:rfc:(\d+)$</id-pattern>
        </series>
        <series id="I-D">
            <name short="I-D">Internet Draft</name>
            <id-pattern url-pattern="http://tools.ietf.org/html/draft-$1" name-pattern="Internet Draft $1" md-pattern="$1">urn:ietf:id:(.+)$</id-pattern>
        </series>
    </organization>
    <organization id="W3C" href="http://www.w3.org/">
        <name short="W3C">World Wide Web Consortium</name>
        <series id="TR">
            <name short="TR">Technical Report</name>
            <id-pattern url-pattern="http://www.w3.org/TR/$1" name-pattern="W3C TR http://www.w3.org/TR/$1" md-pattern="$1">http://www.w3.org/TR/(.+)$</id-pattern>
        </series>
    </organization>
    <organization id="JCP" href="http://jcp.org/">
        <name short="JCP">Java Community Process</name>
        <series id="JSR">
            <name short="JSR">Java Specification Request</name>
            <id-pattern url-pattern="http://www.jcp.org/en/jsr/detail?id=$1" name-pattern="JSR $1" md-pattern="$1">http://www.jcp.org/en/jsr/detail\?id=(\d+)$</id-pattern>
        </series>
    </organization>
    <organization id="ISO" href="http://www.iso.org/">
        <name short="ISO">International Organization for Standardization</name>
        <series id="IEC">
            <name short="IEC">International Electrotechnical Commission</name>
            <id-pattern url-pattern="http://www.iso.org/obp/ui/#iso:std:iso-iec:$1:-$2" name-pattern="ISO/IEC $1" md-pattern="$1-$2">urn:iso:std:iso-iec:(\d+):-(\d+):?.*$</id-pattern>
        </series>
    </organization>
</specs>
