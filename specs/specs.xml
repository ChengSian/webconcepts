<?xml version="1.0" encoding="UTF-8"?>
<specs>
    <organization id="IETF">
        <name short="IETF">Internet Engineering Task Force</name>
        <series id="RFC">
            <name short="RFC">Request for Comments</name>
            <uri-pattern url-pattern="http://tools.ietf.org/html/rfc$1">urn:ietf:rfc:(\d+$)</uri-pattern>
        </series>
        <series id="I-D">
            <name short="I-D">Internet Draft</name>
            <uri-pattern url-pattern="http://tools.ietf.org/html/draft-$1">urn:ietf:id:(.+$)</uri-pattern>
        </series>
    </organization>
    <organization id="W3C">
        <name short="W3C">World Wide Web Consortium</name>
        <series id="TR">
            <name short="TR">Technical Report</name>
            <uri-pattern>http://www.w3.org/TR/(.+)</uri-pattern>
        </series>
    </organization>
</specs>
