{"concept":"oauth-parameter","id":"http://webconcepts.info/concepts/oauth-parameters","name-singular":"OAuth Parameter","name-plural":"OAuth Parameters","values":[{"value":"assertion","concept":"oauth-parameter","id":"http://webconcepts.info/concepts/oauth-parameter/assertion","details":[{"description":"The assertion being used as an authorization grant. Specific serialization of the assertion is defined by profile documents.","documentation":"http://tools.ietf.org/html/rfc7521#section-4.1","specification":"http://webconcepts.info/specs/IETF/RFC/7521"}]},{"value":"client_assertion","concept":"oauth-parameter","id":"http://webconcepts.info/concepts/oauth-parameter/client_assertion","details":[{"description":"The assertion being used to authenticate the client. Specific serialization of the assertion is defined by profile documents.","documentation":"http://tools.ietf.org/html/rfc7521#section-4.2","specification":"http://webconcepts.info/specs/IETF/RFC/7521"}]},{"value":"client_assertion_type","concept":"oauth-parameter","id":"http://webconcepts.info/concepts/oauth-parameter/client_assertion_type","details":[{"description":"The format of the assertion as defined by the authorization server. The value will be an absolute URI.","documentation":"http://tools.ietf.org/html/rfc7521#section-4.2","specification":"http://webconcepts.info/specs/IETF/RFC/7521"}]},{"value":"code_challenge","concept":"oauth-parameter","id":"http://webconcepts.info/concepts/oauth-parameter/code_challenge","details":[{"description":"Code challenge.","documentation":"http://tools.ietf.org/html/rfc7636#section-4.3","specification":"http://webconcepts.info/specs/IETF/RFC/7636"}]},{"value":"code_challenge_method","concept":"oauth-parameter","id":"http://webconcepts.info/concepts/oauth-parameter/code_challenge_method","details":[{"description":"Code verifier transformation method is \"S256\" or \"plain\". Defaults to \"plain\" if not present in the request.","documentation":"http://tools.ietf.org/html/rfc7636#section-4.3","specification":"http://webconcepts.info/specs/IETF/RFC/7636"}]},{"value":"code_verifier","concept":"oauth-parameter","id":"http://webconcepts.info/concepts/oauth-parameter/code_verifier","details":[{"description":"Code verifier.","documentation":"http://tools.ietf.org/html/rfc7636#section-4.5","specification":"http://webconcepts.info/specs/IETF/RFC/7636"}]}]}